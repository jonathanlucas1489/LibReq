
public class Paciente {
	
	private String cartao;
	private String nome;
	private String bairro;
	
	public Paciente(String cartao, String nome, String bairro) {
		this.cartao = cartao;
		this.nome = nome;
		this.bairro = bairro;
	}

	public String getCartao() {
		return cartao;
	}

	public String getNome() {
		return nome;
	}

	public String getBairro() {
		return bairro;
	}

	

	
	
}
